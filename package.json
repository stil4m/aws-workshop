{
  "name": "aws-workshop",
  "version": "0.1.0",
  "bin": {
    "aws-workshop": "bin/aws-workshop.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy-fargate-demo": "cdk deploy DemoFargateStack --require-approval=never",
    "destroy-fargate-demo": "cdk destroy DemoFargateStack --force",
    "deploy-ec2-demo": "cdk deploy DemoEc2Stack --require-approval=never",
    "destroy-ec2-demo": "cdk destroy DemoEc2Stack --force",
    "deploy-s3-demo": "cdk deploy DemoS3Stack --require-approval=never",
    "destroy-s3-demo": "cdk destroy DemoS3Stack --force"
  },
  "devDependencies": {
    "@aws-cdk/aws-ec2": "^1.19.0",
    "@aws-cdk/aws-ecs": "^1.19.0",
    "@aws-cdk/aws-ecs-patterns": "^1.19.0",
    "@aws-cdk/aws-s3": "^1.19.0",
    "@aws-cdk/aws-s3-deployment": "^1.19.0",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.19.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.19.0",
    "source-map-support": "^0.5.16"
  }
}
